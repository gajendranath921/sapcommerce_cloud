<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
                   id="com.hybris.backoffice.widgets.surveylauncher">
    <name>Survey Launcher Widget</name>
    <description>Collect user feedback about Backoffice via qualtrics survey.</description>
    <author>hybris</author>
    <version>1.0</version>
    <view src="surveylauncher.zul" />
    <controller class="com.hybris.backoffice.widgets.surveylauncher.SurveyLauncherController"/>
    <sockets>
        <input id="perspectiveSelected" type="com.hybris.backoffice.navigation.NavigationNode"/>
    </sockets>
    <keywords>
        <keyword>Survey</keyword>
        <keyword>Qualtrics</keyword>
    </keywords>
</widget-definition>
